@model IEnumerable<ShuttleServiceManagementSystem.Models.AccountActivityViewModel>

@{
    ViewBag.Title = "Account Log-In Activity";
}

<h2>@ViewBag.Title for <b>@ViewBag.UserName</b></h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.loginDateTime)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.loginDateTime)
        </td>
    </tr>
}

</table>

<div>
    @Html.ActionLink("Manager Home", "Home", "Managers")
</div>